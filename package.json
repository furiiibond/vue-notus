    "@vue/compiler-sfc": "^3.0.7",
    "chart.js": "2.9.4",
    "core-js": "3.9.1",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8",
    "mosha-vue-toastify": "^1.0.11",
    "tailwindcss": "2.0.4",
    "v-easy-dialog": "^1.0.6",
    "vue": "^3.0.7",
    "vue-form-generator": "^2.3.4",
    "vue-loading-overlay": "^4.0.1",
